{
    "name": "emiliojva/framework-inovuerj",
    "description": "MicroFramework para atender demandas de projetos curtos, do Departamento de Inovação da UERJ. Inovuerj",
    "homepage": "http://inovuerj.sr2.uerj.br",
    "authors": [
        {
            "name": "Emilio Vieira",
            "email": "emilio@sr2.uerj.br"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.0.0"
        }
    },
    "require": {
        "php": ">=7.0",
        "illuminate/support": "v5.5.44",
        "emiliojva/package-patterns": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Inovuerj\\": "vendor/emiliojva/package-patterns",
            "App\\": "App/"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.4"
    }
}